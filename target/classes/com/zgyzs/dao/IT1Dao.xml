<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zgyzs.dao.IT1Dao">
    <select id="findAll" resultType="com.zgyzs.domain.T1">
        select * from t1
    </select>
    <insert id="saveT1" parameterType="com.zgyzs.domain.T1">
        insert into t1(id,k) values(#{id},#{k})
    </insert>
</mapper>